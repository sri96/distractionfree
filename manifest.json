{
  // Required
  "manifest_version": 2,
  "name": "DistractionFree",
  "version": "0.1",
  "description": "An extension to prevent you from getting distracted while studying",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Stop Getting Distracted, Start Being Productive"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
